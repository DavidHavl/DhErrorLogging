        "zendframework/zend-http": "^2.3 || ^3.0",
        "zendframework/zend-modulemanager": "^2.3 || ^3.0",
        "zendframework/zend-config": "^2.3 || ^3.0",
        "zendframework/zend-log": "^2.3 || ^3.0",
        "zendframework/zend-db": "^2.3 || ^3.0",
        "zendframework/zend-view": "^2.3 || ^3.0",
        "zendframework/zend-console": "^2.3 || ^3.0",
        "zendframework/zend-json": "^2.3 || ^3.0"
    },
    "license" : "MIT",
    "authors" : [
        {
            "name" : "David Havl",
            "email" : "contact@davidhavl.com",
            "homepage" : "http://davidhavl.com",
            "role" : "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "DhErrorLogging\\": "src/"
        },
        "classmap" : [
            "./Module.php"
        ]
    }
}