    "type" : "library",
    "keywords": [
        "zend framework",
        "zend framework 2",
        "zend framework 3",
        "zf2",
        "zf3",
        "php-error",
        "exception",
        "error",
        "error log",
        "logger"
    ],
    "homepage" : "https://github.com/DavidHavl/DhErrorLogging",
